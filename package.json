  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow-models/posenet": "^2.2.1",
    "@tensorflow/tfjs": "~1.7.4",
    "@types/dat.gui": "^0.7.5",
    "@types/stats.js": "^0.17.0",
    "stats.js": "^0.17.0"
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development parcel index.html Home.html Loading.html --no-hmr --open ",
