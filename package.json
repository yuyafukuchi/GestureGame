    "dat.gui": "^0.7.2",
    "eslint": "^7.0.0",
    "eslint-config-google": "^0.14.0",
    "parcel-bundler": "~1.12.4",
    "typescript": "^3.9.3",
    "yalc": "~1.0.0-pre.27"
  },
  "eslintConfig": {
    "extends": "google",
    "rules": {
